              img(src="./assets/img/NazarBanner/nice.png" alt="Nice")
        .nr-banner__block 
          +link({href,classes:"nr-banner__recipes",text: "Hot Recipes"})
          h1.nr-banner__title Spicy delicious chicken wings 
          .nr-banner__description Lorem ipsum dolor sit amet, consectetuipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqut enim ad minim 
          .nr-banner__info 
            .nr-banner__info-food
              svg(class="icon icon-Timer")
                use(xlink:href="#icon-Timer")
              .nr-banner__desc 30 Minutes
            .nr-banner__info-food
              svg(class="icon icon-ForkKnife")
                use(xlink:href="#icon-ForkKnife")
              .nr-banner__desc Chicken
          .nr-banner__data
            .nr-banner__author 
              .nr-banner__photo-block
                img.nr-banner__photo(src="./assets/img/NazarBanner/author.png" alt="Photo")
              .nr-banner__author-information 
                .nr-banner__author-name John Smith
                .nr-banner__author-date 15 March 2022
            .nr-banner__button-block
              button.btn.simptip-position-top.simptip-info.e-button.e-button--banner(type="button" data-tooltip="Watch the videos")
                span.e-button__banner-text View Recipes
                svg(class="icon icon-Play")
                  use(xlink:href="#icon-Play")
        .nr-banner__image-block
          picture 
            source(srcset="./assets/img/NazarBanner/dish-banner-1024px.jpg" media="(min-width:769px) and (max-width:1024px)")
            source(srcset="./assets/img/NazarBanner/dish-banner-768px.jpg" media="(max-width:768px)")
            img.nr-banner__image-banner(src="./assets/img/NazarBanner/dish-banner.jpg" alt="Dish")